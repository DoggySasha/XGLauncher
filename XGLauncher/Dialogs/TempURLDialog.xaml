<Window x:Class="XGLauncher.Dialogs.TempURLDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XGLauncher.Dialogs"
        mc:Ignorable="d" Style="{StaticResource XGLauncherDialog}"
        WindowStartupLocation="CenterScreen"
        Title="URL Dialog" Height="140" Width="400">
    <Grid>
        <TextBlock Name="imageURLT" Text="Image (URL)" VerticalAlignment="Top" Margin="5, 15, 5, 5"/>
        <TextBox Name="imageURL" VerticalAlignment="Top" Margin="5, 40, 5, 5" Height="30"/>
        <Button Name="save" Click="PublishImage" VerticalAlignment="Bottom" Margin="5" Height="30"
                Style="{StaticResource BigButtonGray}" HorizontalAlignment="Right" Width="120"
                Content="Save"/>
    </Grid>
</Window>
